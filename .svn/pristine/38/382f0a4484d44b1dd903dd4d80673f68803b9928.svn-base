.flour{
	height: 450px;
	width: 650px;
	outline: 2px solid background;
	position: relative;
	left: 0px;
	top: 0px;
	background-color: red;
/*	overflow: hidden;*/
}
.body4img ul{
	width: 2600px;
	height: 450px;
	animation: move 10s infinite;

}
.body4img{
	width: 650px;
	height: 450px;
	overflow: hidden;
	/*background-color: black;*/
	position: relative;
	left: 405px;
	top: 340px;
	background-image: url(../images/zhaopin/img16.jpg);
/*	background-image: center;*/
    background-size: 650px 450px;
}
@keyframes move{
	10%{
		margin-left: 0
	}
	20% {
		margin-left: 0
	}
	30%{
		margin-left: -650px;
	}
	40%{
		margin-left: -650px;
	}
	50%{
		margin-left: -1300px;
	}
	60%{
		margin-left: -1300px;
	}
	/*50%{
		margin-left: -1950px;
	}*/
	70%{
		margin-left: -1950px;
	}
	80%{
		margin-left: -1950px;
	}
	90%{
		margin-left: -2600px;
	}
	100%{
		margin-left: -2600px;
	}

}
